{
    "name": "jetimob/iugu-sdk-php-laravel",
    "description": "Laravel SDK for Iugu's API",
    "keywords": ["sdk", "laravel", "php", "iugu"],
    "type": "library",
    "authors": [
        {
            "name": "Alexandre Medina",
            "email": "contato@medinaalexandre.dev",
            "role": "Developer"
        },
        {
            "name": "Jetimob",
            "email": "contato@jetimob.com",
            "homepage": "https://jetimob.com"
        }
    ],
    "require": {
        "php": "8.0",
        "ext-json": "*",
        "jetimob/http-php-laravel": "^1.4.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "illuminate/console": "^6.0 || ^7.0 || ^8.0",
        "illuminate/container": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jetimob\\Iugu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetimob\\Iugu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jetimob\\Iugu\\IuguServiceProvider"
            ],
            "aliases": {
                "Iugu": "Jetimob\\Iugu\\Facades\\Iugu"
            }
        }
    },
    "support": {
        "issues": "https://github.com/jetimob/iugu-sdk-php-laravel/issues"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
